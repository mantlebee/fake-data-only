  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rollup": "^4.21.2",
    "rollup-plugin-cleaner": "^1.0.0",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "typedoc": "^0.26.6",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@mantlebee/ts-core": "^0.21.0",
